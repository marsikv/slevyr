<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Slevyr.WebAppHost.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
      <section name="SledovaniVyrobyWebAppHost.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="SledovaniVyrobyWebAppHost.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <!--<add key="www.rootDir" value="WWW" />-->
    <add key="www.rootDir" value="C:\devel\apam\slevyr\Slevyr.WebAppHost\WWW" />
    <add key="WebAppPort" value="5000" />
    <add key="UseLocalHost" value="False" />
    <add key="JsonFilePath" value="data" />
    <add key="DbFilePath" value="sqlite" />
    <!-- pokud ma pracovat jako služba tak musí být cesty absolutní-->
    <!--<add key="JsonFilePath" value="c:\devel\apam\deploy\data" />-->
    <!--<add key="DbFilePath" value="c:\devel\apam\deploy\sqlite" />-->
    <add key="Port" value="COM4" />
    <add key="BaudRate" value="19200" />
    <!-- UseDataReceivedEvent - pro cteni dat z portu pouzivan dataReceived event-->
    <add key="UseDataReceivedEvent" value="false" />
    <!-- ReceivedBytesThreshold ma smysl jen pokud je UseDataReceivedEvent-->
    <add key="ReceivedBytesThreshold" value="11" />
    <!-- nyni uz nema smysl, musi byt vzdy true-->
    <add key="IsRefreshTimerOn" value="True" />
    <!-- cekame [ms] nez se posle dalsi pozadavek, v jinem rezimu nez OldSyncMode ceka jen po send -->
    <add key="RelaxTime" value="300" />
    <!-- doba [ms] po kterou spi worker pred tim nez nacte stav dalsi jednotky-->
    <!--<add key="WorkerSleepPeriod" value="300" />-->
    <!-- timeout [ms] pro nacitani potvrzeni odeslaneho prikazu -->
    <add key="SendCommandTimeOut" value="500" />
    <!-- timeout [ms] pro nacitani dat ktere maji prijit po odeslanem prikazu -->
    <add key="ReadResultTimeOut" value="3000" />
    <!-- urcuje zda se nacita posledni cas OK a NG -->
    <add key="IsReadOkNgTime" value="True" />
    <!-- perioda pro refresh obsahu stránek -->
    <add key="RefreshTimerPeriod" value="5000" />
    <!-- příkazy na jednu adresu jednotky by nemely jít častěji jak jednou za cca 600ms, poté dochází k zahlcení bufferu-->
    <add key="MinCmdDelay" value="600" />
    <!-- urcuje max. pocet pokusu ktere provadi send -->
    <add key="SendAttempts" value="3" />
    <!-- po odeslání příkazu pak čeká na příslušný response -->
    <add key="IsWaitCommandResult" value="true" />
    <!--<add key="UnitAddrs" value="100;101;102;103"/>-->
    <add key="UnitAddrs" value="100;101" />
    <!-- spousti "mockup" rezim bez skutecne komunikace s jednotkami-->
    <add key="MockupMode" value="False" />
    <!-- zapíná Web API rozhraní, kdyz je false stale zapisuje do databaze -->
    <add key="StartWebApi" value="True" />
    <!-- zapíná přístup na online Web API dokumentaci, na adrese localhost:5000/swagger -->
    <add key="StartSwagger" value="True" />
    <!-- výchozí hodnota pro export do souboru -->
    <add key="DefaultExportFileName" value="c:\tmp\export.csv" />
    <!-- znak pro separator desetinne casti v CSV exportu, pokud neni tento parametr uveden je pouzit znak carka -->
    <add key="DecimalSeparator" value="," />
    <!-- seznam uzivatelu - role Admin-->
    <add key="AdminUsers" value="apam;vlama" />
    <!-- seznam uzivatelu - role Udrzba-->
    <add key="MaintenanceUsers" value="ferda" />
    <!-- seznam uzivatelu ve forme jmeno-heslo, pokud neni tento parametr uveden tak autentizace neprobiha -->
    <add key="Users" value="vlama-202cb962ac59075b964b07152d234b70;apam-33fe15bd55c12296ef9a96e4e63af5ba;ferda-2510c39011c5be704182423e3a695e91" />
    <!--<add key="ClientSettingsProvider.ServiceUri" value="" />-->
  </appSettings>
  <runtime>
